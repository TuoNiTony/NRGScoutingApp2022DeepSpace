<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="NRGScoutingApp2022DeepSpace.MainPage"
    xmlns:local="clr-namespace:NRGScoutingApp2022DeepSpace.Views"
    Title="Main Page">

    <NavigationPage Title="Match Entries" IconImageSource="{OnPlatform 'icon_notes.png', iOS='icon_notes_ios.png', MacCatalyst='icon_notes_ios.png'}">
        <x:Arguments>
            <local:AllMatchEntriesPage Title="Match Entries" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="About" IconImageSource="{OnPlatform 'icon_about.png', iOS='icon_about_ios.png', MacCatalyst='icon_about_ios.png'}">
        <x:Arguments>
            <local:AboutPage Title="About" />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>